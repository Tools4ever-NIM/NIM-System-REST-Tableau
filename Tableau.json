                        },
                        "processing_options": {
                            "output_field": "users.user"
                        },
                        "query_parameters": {
                        }
                    }
                },
                "parameters": {},
                "key": "id",
                "resources": {
                    "externalAuthUserId": "string*",
                    "id": "string*",
                    "lastLogin": "string*",
                    "name": "string*",
                    "siteRole": "string*",
                    "fullName": "string*",
                    "authSetting": "string*",
                    "language": "string*",
                    "locale": "string*"
                }
            },
            "Groups": {
                "operations": {
                    "GroupsList": {
                        "method": "get",
                        "call": {
                            "path": "/api/{api_version}/sites/{site_id}/groups",
                            "post_processing": "child_tables"
                        },
                        "processing_options": {
                            "output_field": "groups.group"
                        },
                        "query_parameters": {
                        }
                    }
                },
                "parameters": {},
                "key": "id",
                "resources": {
                    "domain": {
