{
    "schema": {
        "options": {},
        "crud_objects": {
            "Users": {
                "operations": {
                    "UsersList": {
                        "method": "get",
                        "call": {
                            "path": "/api/api-version/sites/{site_id}/users"
                        },
                        "processing_options": {
                            "output_field": "users.user"
                        },
                        "query_parameters": {
                        }
                    }
                },
                "parameters": {},
                "key": "id",
                "resources": {
                    "externalAuthUserId": "string*",
                    "id": "string*",
                    "lastLogin": "string*",
                    "name": "string*",
                    "siteRole": "string*"
                }
            }
        }
    },
    "rest": {
        "config": {
            "baseUrl": "https://{tenant_id}",
            "get": {
                "maxPageCount": 5,
                "query_parameters": {
                },
                "pagination":{
                    "mode":"generic",
                    "path":[],
                    "params":{
                        "page":"{page_number}",
                        "pagesize":"{page_size}"
                    }
                }
            },
            "authentication": "custom_post_token",
            "call_handling": "generic",
            "test_connection": {
                "url": "/api/api-version/sites/{site_id}/users?pageSize=1&pageNumber=1"
            }
        },
        "authOptions": {
            "authUrl": "https://{tenant_id}/api/3.9/auth/signin",
            "postData": {
                "credentials": {
                    "name": "{user_name}",
                    "password": "{password}"
                }
            },
            "directToken": true,
            "tokenPath": "credentials.token",
            "headerName": "X-Tableau-Auth",
            "headerTemplate": "{token}",
            "dataToVariables": {
                "site_id": "credentials.site.id"
            }
        }
    },
    "connection": {
        "items": [
            {
                "name": "tenant_id",
                "type": "textbox",
                "label": "Tenent URL:",
                "value": ""
            },
            {
                "name": "content_url",
                "type": "textbox",
                "label": "Content URL:",
                "value": ""
            }
        ]
    }
}
