                    },
                    "id": "string*",
                    "name": "string*",
                    "import": {
                        "domainName": "string*",
                        "siteRole": "string*",
                        "grantLicenseMode": "string*"
                    }
                }
            },
            "GroupUsers": {
                "operations": {
                    "GroupsList": {
                        "method": "get",
                        "call": {
                            "mode": "iteration",
                            "table": "Groups",
                            "iterator": "id",
                            "path": "/api/{api_version}/sites/{site_id}/groups/{iterator}/users",
                            "base": {
                                "group_id": "{iterator}"
                            }
                        },
                        "processing_options": {
                            "output_field": "users.user"
                        },
                        "query_parameters": {
                        }
                    }
