                "parameters": {},
                "key": "id",
                "resources": {
                    "externalAuthUserId": "string*",
                    "id": "string*",
                    "lastLogin": "string*",
                    "name": "string*",
                    "siteRole": "string*",
                    "locale": "string*",
                    "language": "string*"
                }
            }
        }
    },
    "rest": {
        "config": {
            "baseUrl": "https://{tenant_id}",
            "get": {
                "query_parameters": {
                },
                "pagination":{
                    "mode":"generic",
                    "path":[],
                    "params":{
                        "pageNumber":"{page_number}",
                        "pageSize":"{page_size}"
                    }
                }
            },
            "authentication": "custom_post_token",
            "call_handling": "generic",
            "test_connection": {
                "url": "/api/{api_version}/sites/{site_id}/users?pageSize=1&pageNumber=1"
            }
        },
        "authOptions": {
            "authUrl": "https://{tenant_id}/api/{api_version}/auth/signin",
            "postData": {
                "credentials": {
                    "name": "{user_name}",
                    "password": "{password}",
                    "site": {
                        "contentUrl": ""
                    }
                }
            },
            "directToken": true,
            "tokenPath": "credentials.token",
            "headerName": "X-Tableau-Auth",
            "headerTemplate": "{token}",
            "dataToVariables": {
                "site_id": "credentials.site.id"
            }
        }
    },
    "connection": {
        "items": [
            {
                "name": "tenant_id",
                "type": "textbox",
                "label": "Tenent URL:",
                "value": ""
            },
            {
                "name": "api_version",
                "type": "textbox",
